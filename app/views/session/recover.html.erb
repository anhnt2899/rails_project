<h1>Recover</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: reset_path) do |f| %>
      <h3>Email</h3>
      <%= f.email_field :email, class: "form-control", required: true %>  
      <%= f.submit "submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

